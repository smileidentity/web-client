  "devDependencies": {
    "@cypress/code-coverage": "^3.12.1",
    "cypress": "^13.3.0",
    "eslint": "^8.50.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-cypress": "^2.15.1",
