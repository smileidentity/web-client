{
  "name": "@smileid/web-components",
  "version": "2.0.2",
  "private": "true",
  "main": "dist/main.js",
  "module": "dist/main.js",
  "types": "dist/types/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/main.d.ts",
      "import": "./dist/main.js",
      "require": "./dist/main.js"
    },
    "./combobox": {
      "import": "./dist/components-combobox.js"
    },
    "./document-capture": {
      "import": "./dist/components-document.js"
    },
    "./end-user-consent": {
      "import": "./dist/components-end-user-consent.js"
    },
    "./navigation": {
      "import": "./dist/components-navigation.js"
    },
    "./selfie-capture": {
      "import": "./dist/components-selfie.js"
    },
    "./signature-pad": {
      "import": "./dist/components-signature-pad.js"
    },
    "./totp-consent": {
      "import": "./dist/components-totp-consent.js"
    },
    "./smart-camera-web": {
      "types": "./dist/types/components/smart-camera-web/src/SmartCameraWeb.d.ts",
      "import": "./dist/components-smart-camera-web-src-SmartCameraWeb.js"
    }
  },
  "files": [
    "./lib/components/",
    "./lib/domain/",
    "./lib/styles/",
    "./dist/",
    "./README.md",
    "package.json"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "preview": "vite preview",
    "prepublishOnly": "npm run build",
    "lint:fix": "eslint . --ext .js,.ts,.tsx --fix",
    "lint:html": "npx prettier --write $(git ls-files '*.html')",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "start": "vite build --watch",
    "test": "npx cypress run",
    "type-check": "tsc --noEmit"
  },
  "description": "A collection of Web Components used by SmileID",
  "keywords": [
    "Web Components"
  ],
  "type": "module",
  "author": "SmileID <support@usesmileid.com> (https://usesmileid.com)",
  "dependencies": {
    "@mediapipe/tasks-vision": "^0.10.22-rc.20250304",
    "@preact/signals": "^2.1.1",
    "@tabler/icons-preact": "^3.34.0",
    "preact": "^10.26.9",
    "preact-custom-element": "^4.3.0",
    "preact-router": "^4.1.2",
    "signature_pad": "^5.0.2",
    "validate.js": "^0.13.1"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.10.2",
    "@types/node": "^20.11.24",
    "@types/preact-custom-element": "^4.0.4",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "cross-env": "^7.0.3",
    "cypress": "^13.15.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-prettier": "^5.2.1",
    "glob": "^10.3.10",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.1",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-tsconfig-paths": "^1.4.1"
  }
}
