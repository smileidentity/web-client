{
  "name": "@smileid/web-components",
  "version": "1.4.5-beta",
  "private": "true",
  "main": "./dist/cjs/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./cjs/index.js"
    },
    "./combobox": {
      "import": "./components/combobox/src/index.js",
      "require": "./dist/cjs/components/combobox/src/index.js"
    },
    "./document-capture": {
      "import": "./dist/components/document/src/index.js",
      "require": "./dist/cjs/components/document/src/index.js"
    },
    "./end-user-consent": {
      "import": "./dist/components/end-user-consent/src/index.js",
      "require": "./dist/cjs/components/end-user-consent/src/index.js"
    },
    "./navigation": {
      "import": "./dist/components/navigation/src/index.js",
      "require": "./dist/cjs/components/navigation/src/index.js"
    },
    "./selfie-capture": {
      "import": "./dist/components/selfie/src/index.js",
      "require": "./dist/cjs/components/selfie/src/index.js"
    },
    "./signature-pad": {
      "import": "./dist/components/signature-pad/src/index.js",
      "require": "./dist/cjs/components/signature-pad/src/index.js"
    },
    "./totp-consent": {
      "import": "./dist/components/totp-consent/src/index.js",
      "require": "./dist/cjs/components/totp-consent/src/index.js"
    },
    "./smart-camera-web": {
      "import": "./dist/components/smart-camera-web/src/SmartCameraWeb.js",
      "require": "./dist/cjs/components/smart-camera-web/src/SmartCameraWeb.js"
    }
  },
  "scripts": {
    "build": "NODE_ENV=production node esbuild.js",
    "build:dev": "NODE_ENV=development node esbuild.js",
    "clean": "rm -rf build dist",
    "prepublishOnly": "npm run build",
    "lint:fix": "eslint . --ext .js --fix",
    "lint:html": "npx prettier --write $(git ls-files '*.html')",
    "lint": "eslint . --ext .js",
    "start": "npm run build:dev && npx serve -p 8000 dist",
    "test": "npx cypress run"
  },
  "description": "A collection of Web Components used by SmileID",
  "keywords": [
    "Web Components"
  ],
  "type": "module",
  "author": "SmileID <support@usesmileid.com> (https://usesmileid.com)",
  "dependencies": {
    "signature_pad": "^5.0.2",
    "validate.js": "^0.13.1"
  },
  "devDependencies": {
    "cypress": "^13.15.0",
    "esbuild": "^0.24.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3"
  }
}
